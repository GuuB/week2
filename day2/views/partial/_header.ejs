  <h1 style="text-align:center; font-size:50px">URL shortener</h1>
  <% if (!database['user_id']) { %>
    <h1 style="font-size:20px; text-align:right"><a href="/login">SIGN IN</a></h1>
    <h1 style="font-size:20px; text-align:right"><a href="/register">REGISTER</a></h1>
  <% } else {%>
    <p style="text-align:right">USER: <a style="color:red"><%=database['user_id']%></a></p>
    <form method="POST" action="/logout"style="text-align:right">
      <input type="submit" value="Log Out" style="width:60px">
    </form>
  <% }%>
<br/>
