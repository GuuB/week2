<% for (let url in database['urls']) {%>
  <li>
    <a href="/urls/<%=url%>"><%= url %>
    </a>: <%=database['urls'][url]%>
      <form method="POST" action="/urls/<%=url%>/delete">
          <input type="submit" value="Delete" style="width:60px">
      </form>
      <form method="GET" action="/urls/<%=url%>">
          <input type="submit" value="Edit" style="width:60px">
      </form>
  </li>

<% }%>
